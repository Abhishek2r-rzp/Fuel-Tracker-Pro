{
  "buildCommand": "npm install && npm run build",
  "framework": null,
  "rewrites": [
    {
      "source": "/fuel-tracker/:path*",
      "destination": "/apps/fuel-tracker/dist/:path*"
    },
    {
      "source": "/expense-tracker/:path*",
      "destination": "/apps/expense-tracker/dist/:path*"
    },
    {
      "source": "/:path*",
      "destination": "/apps/host/dist/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, must-revalidate"
        }
      ]
    }
  ]
}
